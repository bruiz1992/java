package pruebas;
import java.awt.BorderLayout;
import java.awt.FlowLayout;

import javax.swing.BoxLayout;
import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.JPanel;

public class prueba1 {
	public prueba1() {
	}

	public static void main(String[] args) {
	
		JFrame.setDefaultLookAndFeelDecorated(true);
		JFrame frame = new JFrame("Flow layot itaca");
		frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
		
		
		JButton jb1 = new JButton("uno");
		JButton jb2 = new JButton("dos");
		JButton jb3 = new JButton("tercer");
		JButton jb4 = new JButton("cuatro");
		JButton jb5 = new JButton("quinto");
		JButton jb6 = new JButton("miau");
		
		
		JPanel panel = new JPanel();
		panel.setLayout(new BoxLayout(panel, 3));
		
		panel.add(jb1);
		panel.add(jb2);
		panel.add(jb3);
		panel.add(jb6);
		panel.add(jb4);
		panel.add(jb5);
		
		frame.getContentPane().add(panel);
		frame.pack();
		frame.setVisible(true);
	}

}
